{
	"categories": [
		{
			"title": "## 🚀 Features",
			"labels": [
				"feature",
				"feat"
			]
		},
		{
			"title": "## 🐛 Fixes",
			"labels": [
				"fix",
				"bugfix"
			]
		},
		{
			"title": "## 📚  Documentation",
			"labels": [
				"docs",
				"documentation"
			]
		},
		{
			"title": "## 📍  Other",
			"labels": [
				"refactor",
				"revert",
				"build",
				"ci"
			]
		}
	],
	"ignore_labels": [
		"ignore"
	],
	"sort": "ASC",
	"template": "${{CHANGELOG}}\n\n**Full Changelog**: https://github.com/sstelfox/obsidian-advanced-slides-nospam/compare/${{FROM_TAG}}...${{TO_TAG}}",
	"pr_template": "- ${{TITLE}}\n",
	"empty_template": "- no changes",
	"label_extractor": [
		{
			"pattern": "^(feat|feature|fix|bugfix|docs|documentation|refactor|revert|build|ci):(.*)",
			"target": "$1"
		}
	],
	"duplicate_filter": {
		"pattern": "\\[ABC-....\\]",
		"on_property": "title",
		"method": "match"
	}
}
